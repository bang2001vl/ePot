<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ButtonNumberNotification">
        <attr name="background_number" format="color"/>
        <attr name="textColor_number" format="color"/>
        <attr name="text_number" format="integer"/>
    </declare-styleable>

    <declare-styleable name="SearchBar">
        <attr name="hint" format="string"/>
        <attr name="android:textSize" format="integer"/>
        <attr name="icon" format="reference"/>
    </declare-styleable>


    <declare-styleable name="ListViewItemWithIcon">
        <attr name="icon_left" format="reference"/>
        <attr name="icon_right" format="reference"/>
        <attr name="text_tittle" format="string"/>
        <attr name="text_hint" format="string"/>
        <attr name="icon_size" format="enum"/>
    </declare-styleable>
</resources>